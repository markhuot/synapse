{
    "name": "markhuot/synapse",
    "description": "A neural-level connection between JavaScript and PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "markhuot",
            "email": "mark@markhuot.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Synapse\\": "php/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "php/tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Synapse\\Laravel\\SynapseServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.7"
    },
    "require": {
        "laravel/framework": "^11|^12"
    }
}
